{
    "name": "arunfung/laravel-apollo",
    "description": "a apollo client for laravel.",
    "keywords": ["laravel", "lumen", "apollo", "php", "php-apollo", "apollo-client"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/arunfung/laravel-apollo/issues",
        "source": "https://github.com/arunfung/laravel-apollo"
    },
    "authors": [
        {
            "name": "Arun Fung",
            "email": "arunfung@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "arunfung/php-apollo": "^0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ArunFung\\LaravelApollo\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArunFung\\LaravelApollo\\ApolloServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
